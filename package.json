    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "bdr": "^1.3.4",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "standard-version": "^4.3.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  }
