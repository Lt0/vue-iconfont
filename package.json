  "babel": {
    "presets": [
      [
        "@bdr/bdr",{
          "env": {
            "modules": false
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@fir-ui/eslint-config-fir": "^0.3.2",
